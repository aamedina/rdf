dpvo-gdpr:A45-3 a owl:Class ;
    rdfs:label "Art 45(3) adequacy decision"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Personal data can flow freely from the EU to a third country with an Adequacy Decision without any further safeguard being necessary."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_45/par_3/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has Adequacy Decision."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-a a owl:Class ;
    rdfs:label "Art 46(2-a) legal instrument"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "A legally binding and enforceable instrument between public authorities or bodies"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_a/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

