dpvo-gdpr:A46-2-e a owl:Class ;
    rdfs:label "Art 46(2-e) code of conduct"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "An approved code of conduct pursuant to GDPR Article 40 together with binding and enforceable commitments of the controller or processor in the third country to apply the appropriate safeguards, including as regards individuals´ rights"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_e/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-f a owl:Class ;
    rdfs:label "Art 46(2-f) certification"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "An approved certification mechanism pursuant to GDPR Article 42 together with binding and enforceable commitments of the controller or processor in the third country to appy the appropriate safeguards, including as regards individuals` rights"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_f/oj> ;
