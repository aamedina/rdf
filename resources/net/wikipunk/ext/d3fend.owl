        <modifies rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#CacheMemory"/>
        <rdfs:isDefinedBy>https://whatis.techtarget.com/definition/memory</rdfs:isDefinedBy>
        <rdfs:label>Processor Cache Memory</rdfs:label>
        <rdfs:seeAlso>https://dbpedia.org/page/CPU_cache</rdfs:seeAlso>
    </owl:Class>
    


