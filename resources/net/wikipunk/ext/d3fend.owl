        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#contains"/>
                <owl:someValuesFrom rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#PhysicalAddress"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#contains"/>
                <owl:someValuesFrom rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#VirtualAddress"/>
            </owl:Restriction>
        </rdfs:subClassOf>
