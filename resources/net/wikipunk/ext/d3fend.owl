    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#DNSLookup -->

    <owl:Class rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#DNSLookup">
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalArtifact"/>
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalEvent"/>
        <definition>A Domain Name System (DNS) lookup is a record returned from a DNS resolver after querying a DNS name server.  Typically considered an A or AAAA record, where a domain name is resolved to an IPv4 or IPv6 address, respectively.</definition>
        <rdfs:label>DNS Lookup</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://dbpedia.org/resource/Domain_Name_System"/>
        <rdfs:seeAlso rdf:resource="http://dbpedia.org/resource/List_of_DNS_record_types"/>
        <rdfs:seeAlso>https://schema.ocsf.io/objects/dns_query</rdfs:seeAlso>
    </owl:Class>
    


