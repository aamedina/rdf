        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#OSAPIPrivateFunction"/>
        <rdfs:label>Windows NtCreateFile</rdfs:label>
        <rdfs:seeAlso>https://j00ru.vexillium.org/syscalls/nt/64/</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#WindowsNtCreateMailslotFile -->

    <owl:Class rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#WindowsNtCreateMailslotFile">
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#OSAPICreateFile"/>
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#OSAPIPrivateFunction"/>
        <definition>Creates a special File Object called Mailslot.</definition>
        <rdfs:label>Windows NtCreateMailslotFile</rdfs:label>
        <rdfs:seeAlso>https://j00ru.vexillium.org/syscalls/nt/64/</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#WindowsNtCreateNamedPipeFile -->

