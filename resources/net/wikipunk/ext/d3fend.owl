        <rdfs:label>Software Artifact Server</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://dbpedia.org/resource/Software_repository"/>
        <rdfs:seeAlso rdf:resource="http://dbpedia.org/resource/Artifact_(software_development)"/>
