    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#Assessment -->

    <owl:Class rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#Assessment">
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#D3FENDCatalogThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#author"/>
                <owl:someValuesFrom rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#expectation-rating"/>
                <owl:allValuesFrom>
                    <rdfs:Datatype>
                        <owl:oneOf>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>below</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>exceeded</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>met</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </owl:oneOf>
                    </rdfs:Datatype>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <comment>This is really an assessment result. If we need to model the assessment as a process in the future we will append Result to these classes.</comment>
        <definition>The classification of someone or something with respect to its worth.</definition>
        <rdfs:isDefinedBy rdf:resource="http://wordnet-rdf.princeton.edu/id/05741528-n"/>
        <rdfs:label>Assessment</rdfs:label>
    </owl:Class>
    


