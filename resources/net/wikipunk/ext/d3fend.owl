        <rdfs:label>Move File</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://man7.org/linux/man-pages/man2/rename.2.html"/>
        <skos:altLabel>Rename File</skos:altLabel>
    </owl:Class>
    


