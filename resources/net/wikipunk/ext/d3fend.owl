        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#SymbolicLink"/>
        <definition>An NTFS symbolic link records the path of another file that the links contents should show. Can accept relative paths. SMB networking (UNC path) and directory support added in NTFS 3.1.</definition>
        <rdfs:isDefinedBy rdf:resource="http://dbpedia.org/resource/NTFS_links"/>
        <rdfs:label>NTFS Symbolic Link</rdfs:label>
        <skos:altLabel>NTFS Symlink</skos:altLabel>
    </owl:Class>
    


