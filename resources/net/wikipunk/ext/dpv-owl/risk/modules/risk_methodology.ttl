dpvo-risk:CORAS a owl:NamedIndividual,
        dpvo-risk:RiskManagementMethodology ;
    rdfs:label "CORAS"@en ;
    dct:created "2022-08-18"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "The CORAS method was developed and is supported by SourceForge. It is a method for conducting the analysis and management of security risk. It provides a customised language for modelling threats and risks as well as detailed guidelines explaining how the language should be used to capture and model relevant information during the various stages of the security analysis"@en ;
    dct:source <https://www.enisa.europa.eu/publications/compendium-of-risk-management-frameworks> ;
    rdfs:isDefinedBy dpvo-risk: ;
    sw:term_status "accepted"@en .

dpvo-risk:CRAMM a owl:NamedIndividual,
        dpvo-risk:RiskManagementMethodology ;
