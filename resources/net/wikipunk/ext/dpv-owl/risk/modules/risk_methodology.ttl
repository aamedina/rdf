dpvo-risk:ETSI-TS-102-165-1 a owl:NamedIndividual,
        dpvo-risk:RiskManagementMethodology ;
    rdfs:label "ETSI TS 102 165-1"@en ;
    dct:created "2022-08-18"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "ETSI TS 102 165-1 offers methodology and pro-forma for threat, vulnerability and risk analysis (TVRA). According to ETSI TS 102 165-1, threat vulnerability and risk analysis (TVRA) is used to identify risk to an information system based upon the product of the likelihood of an attack and the impact that such an attack will have on the system"@en ;
    dct:source <https://www.enisa.europa.eu/publications/compendium-of-risk-management-frameworks> ;
    rdfs:isDefinedBy dpvo-risk: ;
    sw:term_status "accepted"@en .

