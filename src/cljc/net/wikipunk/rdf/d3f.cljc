   :rdf/type #{:owl/NamedIndividual :owl/Class},
   :rdfs/label "Account Locking",
   :rdfs/subClassOf #{:d3f/CredentialEviction
                      {:owl/onProperty     :d3f/disables,
                       :owl/someValuesFrom :d3f/UserAccount,
                       :rdf/type           :owl/Restriction}}})

